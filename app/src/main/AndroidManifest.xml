<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myfood_nhduong">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyFood_NHDuong"
        tools:targetApi="31">
        <activity
            android:name="com.example.myfood_nhduong.LoginActivity_NHDuong"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myfood_nhduong.RegisterActivity_nhduong" android:exported="false" />
        <activity android:name="com.example.myfood_nhduong.HomeActivity_NHDuong" android:exported="false" />
        <activity android:name="com.example.myfood_nhduong.FoodListActivity_NHDuong" android:exported="false" />
        <activity android:name="com.example.myfood_nhduong.FoodDetailActivity_NHDuong" android:exported="false" />
        <activity android:name="com.example.myfood_nhduong.MainActivity" android:exported="false" />
    </application>

</manifest>